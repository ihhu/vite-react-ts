{
  "name": "react-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite -c ./build/vite.config.ts",
    "build": "tsc && vite build -c ./build/vite.config.ts",
    "preview": "vite preview",
    "tsc": "tsc --watch",
